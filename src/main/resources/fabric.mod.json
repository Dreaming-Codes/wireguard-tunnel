{
	"schemaVersion": 1,
	"id": "wireguard-tunnel",
	"version": "${version}",
	"name": "WireGuard Tunnel",
	"description": "A Fabric mod that tunnels Minecraft server connections through WireGuard/WARP, providing encrypted and private networking for your gameplay.",
	"authors": [
		"DreamingCodes"
	],
	"contact": {
		"homepage": "https://github.com/DreamingCodes/wireguard-tunnel",
		"sources": "https://github.com/DreamingCodes/wireguard-tunnel",
		"issues": "https://github.com/DreamingCodes/wireguard-tunnel/issues"
	},
	"license": "GPL-3.0",
	"icon": "assets/wireguard-tunnel/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			"codes.dreaming.wireguard.WireguardTunnel"
		],
		"client": [
			"codes.dreaming.wireguard.WireguardTunnelClient"
		]
	},
	"mixins": [
		"wireguard-tunnel.mixins.json",
		{
			"config": "wireguard-tunnel.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.0",
		"minecraft": "~1.19.2",
		"java": ">=17"
	}
}
